{
  "name": "dhii/pdo-query",
  "description": "A standards-compliant PDO query wrapper.",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Ukhanev",
      "email": "xedin.unknown@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "dhii/query-interface": "^0.1-alpha1",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
    "vimeo/psalm": "^3.11.7",
    "slevomat/coding-standard": "^6.0",
    "ext-pdo_sqlite": "*",
    "dhii/containers": "^0.1.0-alpha1"
  },
  "autoload": {
    "psr-4": {
      "Dhii\\PdoQuery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dhii\\PdoQuery\\Test\\Func\\": "tests/functional"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-initial": "0.1.x-dev"
    }
  }
}
